
<div class= forms>
	<h1>Sign up </h1>

	<form action = "/signup" method="POST">
		<div class="labels"><label>First Name:</label></div>
		<input type= "text" placeholder="first name" name="firstName"></br>

		<div class="labels"><label>Last Name:</label></div>
		<input type= "text" placeholder="last name" name="lastName"></br>

		<div class="labels"><label>E-mail:</label></div>
		<input type= "email" placeholder="email" name="email"></br>

		<div class="labels"><label>Birthdate:</label></div>
		<input type= "date" placeholder="" name="birthday"></br>

		<div class="labels"><label>Password:</label></div>
		<input type= "password" placeholder="Create password" name="password"></br>

		<input type= "submit" value="Sign up">
		<p>Already registered? <a href="/"> Log in.</a></p>
	</form>
</div>

